# See http://editorconfig.org/ for documentation

# top-most EditorConfig file
root = true

# Every file
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

# PHP
[*.php]
indent_style = space
indent_size = 4

# JavaScript
[*.js]
indent_style = space
indent_size = 2

# CSS
[*.css]
indent_style = space
indent_size = 4





